[req]
distinguished_name = req_distinguished_name
req_extensions = v3_req
prompt = no
default_md = sha256
[req_distinguished_name]
C = {{ openssl_cert_country_name }}
ST = {{ openssl_cert_state_name }}
L = {{ openssl_cert_locality_name }}
O = {{ openssl_cert_organization_name }}
OU = {{ openssl_cert_organizational_unit_name }}
CN = {{ openssl_cert_common_name }}
emailAddress = ovndev@visa.com
[v3_req]
subjectAltName = {{ openssl_cert_subject_alt_names | join(',') }}
basicConstraints = CA:FALSE
subjectKeyIdentifier = hash
keyUsage = digitalSignature, nonRepudiation, keyEncipherment, dataEncipherment, keyAgreement
